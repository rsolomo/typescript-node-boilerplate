{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": true,
    "noEmitOnError": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "newLine": "LF",
    "outDir": "dist"
  },
  "exclude": [
    "node_modules"
  ]
}